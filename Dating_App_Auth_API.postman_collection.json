    "event": [
        {
            "listen": "prerequest",
            "script": {
                "type": "text/javascript",
                "exec": [
                    "// Auto-set content type for form data requests",
                    "if (pm.request.body && pm.request.body.mode === 'formdata') {",
                    "    pm.request.headers.remove('Content-Type');",
                    "}"
                ]
            }
        },
        {
            "listen": "test",
            "script": {
                "type": "text/javascript",
                "exec": [
                    "// Auto-save access token from authentication responses",
                    "if (pm.response.json() && pm.response.json().access_token) {",
                    "    pm.environment.set('access_token', pm.response.json().access_token);",
                    "    console.log('Access token saved to environment');",
                    "}",
                    "",
                    "// Log response for debugging",
                    "console.log('Response Status:', pm.response.code);",
                    "console.log('Response Body:', pm.response.text());"
                ]
            }
        }
